                                                    '<td>Lease Term :</td>'+
                                                    '<td><input name="leaseterm" type="text" class="input_pipup21" id="leaseterm" value="'+data['Leaseterm']+'" readonly></td>'+
                                                    '<td>&nbsp;</td>'+
                                                    '<td>&nbsp;</td>'+
                                                  '</tr>'+
                                              '</table></td>'+
                                          
                                            '</tr>'+
                                            '<tr>'+
                                              '<td colspan="2" align="left" scope="row"> <label class="">'+
                                                            '<input type="checkbox" name="CheckboxGroup3" value="Click to Confirm That This is The Correct Address of Residence">'+
                                                            'Click to Confirm That This is The Correct Address of Residence</label></td>'+
                                            '</tr>'+
